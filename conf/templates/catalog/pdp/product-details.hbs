<!-- start catalog/pdp/product-details.hbs -->
<div class="panel panel-default">
  <div class="panel-heading" role="tab" id="headingProductDetails">
    <h4 class="panel-title product-accordion-title text-uppercase">
      <a id="pdp-product-details-toggle" class="collapsed pdp-accord-toggle" data-toggle="collapse" data-parent="#accordion-product-info" href="#collapseProductDetails" aria-expanded="false" aria-controls="collapseProductDetails">{{i18n "catalog:details.title"}}
        <img class="accordion-plus" src="{{@root.meta.assetsPath}}img/plus79-ff2fd999af2fc5d389613530e5c59636.png" alt="accordion content">
      </a>
    </h4>
  </div>
  <div id="collapseProductDetails" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingProductDetails">
    <div class="panel-body panel-body-pdp">
      <ul class="product-features-list">
        {{#displayedAttributes content.myvariant}}
            <li><span>{{printAttributeLabel this content.myproduct.productType}}:</span> {{printAttributeValue this content.myproduct.productType}}</li>
        {{/displayedAttributes}}
      </ul>
    </div>
  </div>
</div>
<!-- end catalog/pdp/product-details.hbs -->
